<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
  "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.finary.daily-sync</string>

    <key>ProgramArguments</key>
    <array>
        <string>/Users/sylvain/_MACARON-SOFTWARE/_FINARY/scrapers/.venv/bin/python3</string>
        <string>/Users/sylvain/_MACARON-SOFTWARE/_FINARY/scrapers/daily_sync.py</string>
    </array>

